{
  "name": "building-deterioration",
  "version": "0.0.2",
  "description": "building-deterioration",
  "scripts": {
    "django": "python manage.py runserver 0.0.0.0:$(/bin/cat port 2> /dev/null || /bin/echo 8000)",
    "html": "pug ./index.pug --out dist",
    "js-build": "parcel build ./proj.js --public-url .",
    "js-watch": "parcel watch ./proj.js --public-url .",
    "parcel": "parcel ./index.pug --port $(/bin/cat port 2> /dev/null || /bin/echo 1234)",
    "start": "concurrently 'yarn run django' 'yarn run js-watch'"
  },
  "repository": "zoro:building-deterioration",
  "author": "Tien-Hao Chang <darby.chang@gmail.com>",
  "license": "All rights reserved",
  "private": true,
  "devDependencies": {
    "@vue/component-compiler-utils": "^2.6.0",
    "concurrently": "^4.1.0",
    "parcel": "^1.12.0",
    "pug": "^2.0.3",
    "sass": "^1.17.3",
    "vue-template-compiler": "^2.6.10"
  },
  "dependencies": {
    "pug-cli": "^1.0.0-alpha6",
    "semantic-ui-offline": "^2.4.1",
    "vue": "^2.6.10",
    "vue-hot-reload-api": "^2.3.3",
    "vue-js-modal": "^1.3.28",
    "vue-picture-input": "^2.1.6"
  }
}
